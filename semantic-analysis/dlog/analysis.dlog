 aso:TimeSeries] :- [?TS, rdf:type, aso:TimeSeries-C] .# Add indirectly contained tweets.[?TS, aso:timeSeriesLow, ?TW] :-     [?TS, rdf:type,                aso:TimeSeries-SP],    [?TS, aso:timeSeriesSubject,   ?S],    [?TS, aso:timeSeriesPredicate, ?P],    [?TW, aso:tweetEntity,         ?S],    [?TW, aso:tweetPredicate,      ?P],    NOT [?TS, aso:timeSeriesHigh,  ?TW] .[?TS, aso:timeSeriesLow, ?TW] :-     [?TS, rdf:type,                aso:TimeSeries-PO],    [?TS, aso:timeSeriesPredicate, ?P],    [?TS, aso:timeSeriesObject,    ?O],    [?TW, aso:tweetPredicate,      ?P],    [?TW, aso:tweetEntity,         ?O],    NOT [?TS, aso:timeSeriesHigh,  ?TW] .[?TS, aso:timeSeriesLow, ?TW] :-     [?TS, rdf:type,               aso:TimeSeries-SC],    [?TS, aso:timeSeriesSubject,  ?S],    [?TS, aso:timeSeriesCountry,  ?C],    [?TW, aso:tweetEntity,        ?S],    [?TW, aso:tweetCountry,       ?C],    NOT [?TS, aso:timeSeriesHigh, ?TW] .[?TS, aso:timeSeriesLow, ?TW] :-     [?TS, rdf:type,                aso:TimeSeries-PC],    [?TS, aso:timeSeriesPredicate, ?P],    [?TS, aso:timeSeriesCountry,   ?C],    [?TW, aso:tweetPredicate,      ?P],    [?TW, aso:tweetCountry,        ?C],    NOT [?TS, aso:timeSeriesHigh,  ?TW] .[?TS, aso:timeSeriesLow, ?TW] :-     [?TS, rdf:type,                aso:TimeSeries-SPC],    [?TS, aso:timeSeriesSubject,   ?S],    [?TS, aso:timeSeriesPredicate, ?P],    [?TS, aso:timeSeriesCountry,   ?C],    [?TW, aso:tweetEntity,         ?S],    [?TW, aso:tweetPredicate,      ?P],    [?TW, aso:tweetCountry,        ?C],    NOT [?TS, aso:timeSeriesHigh,  ?TW] .# Generate background subject part of time series[?TS, aso:timeSeriesBackgroundSubject, ?TW] :-     [?TS, rdf:type,              aso:TimeSeries],    [?TS, aso:timeSeriesSubject, ?S],    [?Q, aso:quadSubject,        ?S],    [?TW, aso:tweetQuad,         ?Q] .[?TS, aso:timeSeriesBackgroundSubject, ?TW] :-     [?TS, rdf:type,              aso:TimeSeries],    [?TS, aso:timeSeriesSubject, ?S],    [?TW, aso:tweetEntity,       ?S] .[?TS, aso:timeSeriesBackgroundObject, ?TW] :-     [?TS, rdf:type,             aso:TimeSeries],    [?TS, aso:timeSeriesObject, ?O],    [?Q, aso:quadObject,        ?O],    [?TW, aso:tweetQuad,        ?Q] .[?TS, aso:timeSeriesBackgroundObject, ?TW] :-     [?TS, rdf:type,             aso:TimeSeries],    [?TS, aso:timeSeriesObject, ?O],    [?TW, aso:tweetObject,      ?O] .[?TS, aso:timeSeriesBackgroundCountry, ?TW] :-     [?TS, rdf:type,              aso:TimeSeries],    [?TS, aso:timeSeriesCountry, ?C],    [?Q, aso:quadCountry,        ?C],    [?TW, aso:tweetQuad,         ?Q] .[?TS, aso:timeSeriesBackgroundCountry, ?TW] :-     [?TS, rdf:type,              aso:TimeSeries],    [?TS, aso:timeSeriesCountry, ?C],    [?TW, aso:tweetCountry,      ?C] .# ================================================================================# The following rules identify interesting time series types.# Politicians dying[?TS, rdf:type,                aso:PoliticianDying],[?TS, aso:timeSeriesSubject,   ?S],[?TS, aso:timeSeriesPredicate, wnr:200359085-v],[?TS, aso:timeSeriesHigh,      ?TW] :-    [?TW, aso:tweetQuad,    ?Q],    [?Q, aso:quadSubject,   ?S], [?S, rdf:type, yago:Politician110451263],    [?Q, aso:quadPredicate, wnr:200359085-v],    BIND(SKOLEM("ts-sp", ?S, wnr:200359085-v) AS ?TS) .[?TS, rdf:type, aso:TimeSeries-SP] :- [?TS, rdf:type, aso:PoliticianDying] .# Militia terror acts[wnr:201473673-v, rdf:type, aso:TerrorAct] :- .  # kidnap[wnr:201474455-v, rdf:type, aso:TerrorAct] :- .  # hijack[wnr:201326622-v, rdf:type, aso:TerrorAct] :- .  # kill[wnr:201121587-v, rdf:type, aso:TerrorAct] :- .  # attack[wnr:201134328-v, rdf:type, aso:TerrorAct] :- .  # bomb[wnr:201806671-v, rdf:type, aso:TerrorAct] :- .  # torture[wnr:201215136-v, rdf:type, aso:TerrorAct] :- .  # seize[wnr:201622948-v, rdf:type, aso:TerrorAct] :- .  # destroy[?TS, rdf:type,                aso:MilitiaTerrorAct],[?TS, aso:timeSeriesSubject,   ?S],[?TS, aso:timeSeriesPredicate, ?P],[?TS, aso:timeSeriesHigh,  